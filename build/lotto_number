<script>
$(document).ready(function(){
	$('#generate_number').click(function(){
		$.ajax({
			url:"/get_lotto_number",
			success:function(data){
			    console.log('sucess')
                console.log(data.choice_number)
				$('#num1').html(data.choice_number[0])
				$('#num2').html(data.choice_number[1])
				$('#num3').html(data.choice_number[2])
				$('#num4').html(data.choice_number[3])
				$('#num5').html(data.choice_number[4])
				$('#num6').html(data.choice_number[5])
			}
		});
	})
})
</script>

